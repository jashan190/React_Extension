{
  "manifest_version": 3,
  "name": "Prayer Time Notifier",
  "version": "0.1.0",
  "description": "Get notified for prayer times.",
  "permissions": ["notifications", "storage", "alarms"],
  "host_permissions": [
    "https://api.aladhan.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}


  